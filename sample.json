{
    "Name": "sample",
    "Hosts": {
        "vse": {
            "Host": "http://127.0.0.1:8009",
            "Proxy": ""
        }
    },
    "Messages": {
        "req": {
            "Method": "POST",
            "Path": "/debug/detect/all",
            "Headers": {
                "content-type": "application/json"
            },
            "Body": {
                "image": {
                    "url": "$(IMAGE)"
                },
                "roi": {
                    "X": "`cvt -i $(X)`",
                    "Y": "`cvt -i $(Y)`",
                    "W": "`cvt -i $(W)`",
                    "H": "`cvt -i $(H)`"
                }
            }
        }
    },
    "Tests": {
        "recognize": {
            "PreProcess": [
                "`list /home/gqjiang/project/vsec/depends/res/img.list | envw JSON`",
                "`json .image $(JSON) | envw IMAGE`",
                "`json .roi.[0] $(JSON) | envw X`",
                "`json .roi.[1] $(JSON) | envw Y`",
                "`json .roi.[2] $(JSON) | envw W`",
                "`json .roi.[3] $(JSON) | envw H`"
            ],
            "Host": "vse",
            "Request": "req",
            "Response": {
                "Check": [
                    "`assert $(STATUS) != 200`",
                    "`report`"
                ],
                "Template": null
            },
            "Timeout": "10s"
        }
    },
    "Mode": "",
    "Schedules": [
        {
            "Name": "recog-image",
            "PreProcess": [],
            "Tests": "recognize",
            "Reporter": {
                "Path": "report.log",
                "Format": "{\"err\": $(ERROR), \"status\": $(STATUS), \"response\": $(RESPONSE)}\n"
            },
            "Count": 0,
            "Concurrency": 1,
            "Env": null
        }
    ],
    "Options": {
        "AbortIfFail": "true",
        "Debug": "true"
    }
}