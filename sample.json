{
    "Name": "sample",
    "Hosts": {
        "vse": {
            "Host": "http://127.0.0.1:8009",
            "Proxy": ""
        }
    },
    "Messages": {
        "req": {
            "Method": "POST",
            "Path": "/debug/detect/all",
            "Headers": {
                "content-type": "application/json"
            },
            "Body": {
                "image": {
                    "url": "$(IMAGE)"
                },
                "roi": {
                    "X": "`cvt -i $(X)`",
                    "Y": "`cvt -i $(Y)`",
                    "W": "`cvt -i $(W)`",
                    "H": "`cvt -i $(H)`"
                }
            }
        }
    },
    "Tests": {
        "recognize": {
            "PreProcess": [
                "`list /home/gqjiang/project/vsec/depends/res/img.list | envw JSON`",
                "`json .image $(JSON) | envw IMAGE`",
                "`json .x $(JSON) | envw X`",
                "`json .y $(JSON) | envw Y`",
                "`json .w $(JSON) | envw W`",
                "`json .h $(JSON) | envw H`"
            ],
            "Host": "vse",
            "Request": "req",
            "Response": {
                "Success": [
                    "`report`"
                ],
                "Failure": [
                    "`report`"
                ],
                "Template": null
            },
            "Timeout": "10s"
        }
    },
    "Mode": "",
    "Schedules": [
        {
            "Name": "recog-image",
            "PreProcess": [],
            "Tests": "recognize",
            "Reporter": {
                "Path": "report.log",
                "Format": "{ \"Error\": \"$(ERROR)\", \"Status\": $(STATUS), \"Response\": $(RESPONSE) }\n"
            },
            "Count": 0,
            "Concurrency": 1,
            "Env": null
        }
    ],
    "Options": {
        "AbortIfFail": "true",
        "Debug": "true"
    }
}