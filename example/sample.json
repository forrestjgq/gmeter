{
    "Name": "sample",
    "Hosts": {
        "vse": {
            "Host": "http://127.0.0.1:8009",
            "Proxy": ""
        }
    },
    "Messages": {
        "req": {
            "Method": "POST",
            "Path": "/debug/detect/all",
            "Headers": {
                "content-type": "application/json"
            },
            "Body": {
                "image": {
                    "url": "$(IMAGE)"
                }
            }
        }
    },
    "Tests": {
        "recognize": {
            "PreProcess": [
                "`list /home/gqjiang/project/vsec/depends/res/img.list | envw JSON`",
                "`json .image $(JSON) | envw IMAGE`"
            ],
            "Host": "vse",
            "Request": "req",
            "Response": {
                "Check": [
                    "`assert $(STATUS) == 200`"
                ],
                "Success": [
                    "`report -n -t common`"
                ],
                "Failure": [
                    "`envmv ERROR FAILURE |escape $(FAILURE)| report -n`"
                ],
                "Template": null
            },
            "Timeout": "10s"
        }
    },
    "Mode": "",
    "Schedules": [
        {
            "Name": "recog-image",
            "PreProcess": [],
            "Tests": "recognize",
            "Reporter": {
                "Path1": "report.log",
                "Format": "{ \"Error\": \"$(FAILURE)\" }",
                "Templates": {
                    "common" : { "Error": "", "Status": "`cvt -i $(STATUS)`"}
                }
            },
            "Count": 0,
            "Concurrency": 1,
            "Env": null
        }
    ],
    "Options": {
        "AbortIfFail": "true",
        "Debug": "false"
    }
}