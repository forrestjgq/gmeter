{
  "Servers": {
    "server": {
      "Address":  "127.0.0.1:18001",
      "Routes":  [
        {
          "Method":  "POST",
          "Path":  "/vse",
          "Headers":  {
            "content-type":  "application/json"
          },
          "Request":  {
            "Template": {
              "KafkaResult": "`envw -c $ KAFKA`"
            },
            "Check":  [
              "`json .ObjType $(KAFKA) |envw -c $$ OBJECT`",
              "`json .FmtType $(KAFKA) | envw -c $$ FMT`",
              "`json .BinData $(KAFKA) | b64 -d $$ | envw -c $$ MSG`"
            ],
            "Success":  [
              "`report -n -t common`"
            ],
            "Fail":  []
          }
        }
      ],
      "Report":  {
        "Path": "deepcloud.log",
        "Templates": {
          "common": { "ObjType": "`cvt -i $(OBJECT)`", "FmtType": "`cvt -i $(FMT)`", "BinData": "`cvt -r $(MSG)`" }
        }
      }
    }
  }
}
